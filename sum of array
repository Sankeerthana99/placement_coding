import java.util.Scanner;
class SumOfArray{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int[] arr = new int[n];
        for(int i=0; i<n;i++){
            arr[i] = sc.nextInt();
        }
        System.out.println(Sum(n,arr));
    }
    public static int Sum( int n, int[] arr){
        int count = arr[0];
        for(int i=1; i<arr.length;i++){
            count=count+arr[i];
        }
        return count;
    }
}
